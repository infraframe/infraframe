{
    "name": "sip-agent",
    "dest": "sip_agent",
    "common": {
        "package": "../package.json",
        "files": [
            "agent.toml",
            "log4js_configuration.json",
            "install_deps.sh",
            "configLoader.js",
            "../index.js",
            "../networkHelper.js",
            "../nodeManager.js",
            "../workingNode.js",
            "log4cxx.properties",
            "../../common/clusterWorker.js",
            "../../common/loadCollector.js",
            "../../common/logger.js",
            "../../common/makeRPC.js",
            "../../common/grpcTools.js",
            "../../protocol/protos/protoConfig.json",
            "../../protocol/protos/*.proto",
            "../../../scripts/release/initauth.js"
        ],
        "folders": {
            "sip": [
                "index.js",
                "sipCallConnection.js",
                "grpcAdapter.js",
                "../../common/grpcTools.js",
                "../../protocol/protos/protoConfig.json",
                "../../protocol/protos/*.proto",
                "../connections.js",
                "../internalConnectionRouter.js"
            ]
        }
    },
    "natives": {
        "libdist": "lib",
        "addons": [
            {
                "folder": "sipIn",
                "name": "sipIn"
            },
            {
                "folder": "../addons/mediaFrameMulticaster",
                "name": "mediaFrameMulticaster"
            },
            {
                "folder": "../webrtc/rtcFrame",
                "name": "rtcFrame"
            },
            {
                "folder": "../addons/internalIO",
                "name": "internalIO"
            },
            {
                "folder": "../addons/logger",
                "name": "logger"
            },
            {
                "folder": "../addons/quicIO",
                "name": "quicIO"
            }
        ]
    },
    "start": "node . -U sip"
}
